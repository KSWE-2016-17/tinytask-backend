{
  "name": "tinytask-backend-graphql",
  "version": "0.1.0",
  "description": "GraphQL API for TinyTask.",
  "main": "index.js",
  "scripts": {
    "preserve": "( ping -c 1 google.de && npm -s install ) || exit 0",
    "serve": "nodemon lib/server.js --exec babel-node --presets es2015,stage-0",
    "build": "babel lib -d dist --presets es2015,stage-0",
    "lint": "eslint lib || exit 0",
    "start": "node dist/server.js",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "prebuild": "mkdir -p dist"
  },
  "keywords": [
    "tinytask",
    "api",
    "graphql",
    "mongo",
    "mongodb"
  ],
  "author": "Alexej Esau, Ruben Gees",
  "license": "UNLICENSED",
  "dependencies": {
    "babelify": "7.3.0",
    "body-parser": "1.15.2",
    "browserify": "13.1.1",
    "express": "4.14.0",
    "express-graphql": "0.6.1",
    "graphql": "0.8.2",
    "graphql-compose": "1.5.0",
    "graphql-compose-connection": "2.1.1",
    "graphql-compose-mongoose": "1.3.5",
    "graphql-date": "1.0.2",
    "mongoose": "4.6.8",
    "tinytaskdb": "github:kswe-2016-17/tinytaskdb"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "eslint": "3.10.2",
    "nodemon": "1.11.0"
  }
}
